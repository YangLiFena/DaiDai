<view class="line"></view>
    <view class="viewTop">
      <text class="textTop">订单详情</text>
    </view>
    <view>
      <view>
        <van-field value="{{ tasks.good }}" disabled label="货物简介" input-align="right">
        </van-field>
      </view>
      <view class="section">
        <van-cell title="取货地点" icon="location" value="{{ tasks.from.name }}">
        </van-cell>
      </view>
      <view class="section">
        <van-cell title="送达地点" icon="location" value="{{ tasks.to.name }}">
        </van-cell>
      </view>
      <view class="section">
        <van-cell title="期望送达时间" value="{{ tasks.date }} {{ tasks.time }}">
        </van-cell>
      </view>
      <view>
        <van-field value="{{ tasks.money }} 元 " disabled  label="打赏费用" input-align="right">
        </van-field>
      </view>
      <view>
        <van-field value="{{ tasks.credit }}" disabled  label="最低信誉积分" input-align="right">
        </van-field>
      </view>
      <view class="beizhu">
          <view class="text111">
            <text>订单备注</text>
              <view class="line2"></view>
              <view class="message">
                <text>{{tasks.message}}</text>
              </view>
          </view>
  </view>
    </view>