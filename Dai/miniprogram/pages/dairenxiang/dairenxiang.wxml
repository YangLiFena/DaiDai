<view class="line"></view>
    <view class="viewTop">
      <text class="textTop">订单详情</text>
    </view>
    <view>
      <view class="section">
        <van-cell title="出发地" icon="location" value="{{ tasks1.from.name }}">
        </van-cell>
      </view>
      <view class="section">
        <van-cell title="目的地" icon="location" value="{{ tasks1.to.name }}">
        </van-cell>
      </view>
      <view class="section">
        <van-cell title="出发时间" value="{{ tasks1.date }} {{ tasks1.time }}">
        </van-cell>
      </view>
      <view>
        <van-field value="{{ tasks1.money }} 元 " disabled  label="打赏费用" input-align="right">
        </van-field>
      </view>
      <view>
        <van-field value="{{ tasks1.credit }}" disabled  label="最低信誉积分" input-align="right">
        </van-field>
      </view>
      <view class="beizhu">
          <view class="text111">
            <text>订单备注</text>
              <view class="line2"></view>
              <view class="message">
                <text>{{tasks1.message}}</text>
              </view>
          </view>
  </view>

    </view>